<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".statisticdata.StatisticdailyFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.jnu.yidongzuoye.view.IncomBIll
            android:id="@+id/statistic"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/item_view_border"
            app:layout_constraintBottom_toTopOf="@+id/statistic2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_weight="1"></com.jnu.yidongzuoye.view.IncomBIll>

        <com.jnu.yidongzuoye.view.CustomCurveChart
            android:id="@+id/statistic2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/item_view_border"
            app:layout_constraintBottom_toTopOf="@+id/statistic3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/statistic"
            app:layout_constraintVertical_weight="1"></com.jnu.yidongzuoye.view.CustomCurveChart>

        <com.jnu.yidongzuoye.view.Statistic
            android:id="@+id/statistic3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="@drawable/item_view_border"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/statistic2"
            app:layout_constraintVertical_weight="1"></com.jnu.yidongzuoye.view.Statistic>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>
